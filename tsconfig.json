{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "ES6"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/modules/common/components/*"
      ],
      "@constants/*": [
        "src/modules/common/constants/*"
      ],
      "@context/*": [
        "src/modules/common/context/*"
      ],
      "@helpers/*": [
        "src/modules/common/helpers/*"
      ],
      "@hooks/*": [
        "src/modules/common/hooks/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@services/*": [
        "src/modules/common/services/*"
      ],
      "@styles/*": [
        "src/modules/common/styles/*"
      ],
      "@store/*": [
        "src/modules/common/store/*"
      ],
      "@database/*": [
        "./src/database/*"
      ],
      "@modules/*": [
        "./src/modules/*"
      ],
      "@common/*": [
        "./src/modules/common/*"
      ],
      "@public/*": [
        "./public/*"
      ]
    },
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
